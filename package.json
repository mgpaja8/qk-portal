{
  "name": "react-start",
  "version": "1.0.0",
  "description": "Boilerplate for react and redux",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && webpack-dev-server --content-base public/ --inline --hot",
    "build:prod": "webpack -p",
    "watch:sass": "node-sass-chokidar sass/main.scss -o public/content --watch --recursive"
  },
  "author": "Pavle Milicevic",
  "license": "ISC",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ionicons": "^2.1.6",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "node-sass-chokidar": "0.0.3",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
